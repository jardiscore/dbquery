{
  "name": "jardiscore/dbquery",
  "description": "A flexible package to build sql queries for MySQL/MariaDB, PostgresSQL, and SQLite",
  "version": "1.0.0",
  "type": "library",
  "license": "PolyForm-Noncommercial-1.0.0",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Jardis Development Core",
      "email": "jardiscore@headgent.dev"
    }
  ],
  "require": {
    "php": ">=8.2",
    "jardispsr/dbquery": "^1.0",
    "jardiscore/dotenv": "^1.0",
    "ext-pdo": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "phpstan/phpstan": "^2.0.4",
    "squizlabs/php_codesniffer": "^3.11.2"
  },
  "autoload": {
    "psr-4": {
      "JardisCore\\DbQuery\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JardisCore\\DbQuery\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "echo '#!/usr/bin/env bash' > .git/hooks/pre-commit",
      "echo 'bash ./support/pre-commit-hook.sh' >> .git/hooks/pre-commit",
      "chmod +x .git/hooks/pre-commit"
    ]
  },
  "homepage": "https://github.com/jardiscore/DbQuery",
  "support": {
    "issues": "https://github.com/jardiscore/DbQuery/issues",
    "email": "jardiscore@headgent.dev"
  },
  "keywords": ["database", "pdo", "query", "builder", "sql", "domain driven design", "jardis", "JardisCore", "Headgent"]
}
